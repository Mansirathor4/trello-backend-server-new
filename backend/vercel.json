
{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    
    "github": {
        "silent": true
    },
    "functions": {
        "api/**/*.js": {
            "maxDuration": 60 
        }
    }
}